@prefix : <http://www.purl.org/linked-data/dbcells#> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix interval: <http://reference.data.gov.uk/def/intervals/> .
@prefix sdmx:    <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-measure: <http://purl.org/linked-data/sdmx/2009/measure#> .
@prefix sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#> .

@prefix sweet-distance: <http://sweetontology.net/propSpaceDistance/> .


@prefix dbc-code:   <<http://www.purl.org/linked-data/dbcells/concept#> .
@prefix eg:       <http://example.org/ns#> .

@prefix ex-geo:   <http://example.org/geo#> .

@base <http://www.purl.org/linked-data/dbcells#> .

<http://www.purl.org/linked-data/dbcells#> rdf:type owl:Ontology .


# -- Concepts  ----------------------------

# code list

dbc-code:drivers a skos:ConceptScheme,  sdmx:CodeList;
    rdfs:label "Drivers"@en;
    .


dbc-code:Drivers a rdfs:Class, owl:Class;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Code list for Drivers - codelist class"@en;
    rdfs:comment """This  code list provides ...."""@en;
    rdfs:seeAlso dbc-code:drivers .


dbc-code:roads a skos:Concept, sdmx:Concept, dbc-code:Drivers;
    #rdfs:label "Roads"@en;
    skos:topConceptOf dbc-code:drivers ;
    skos:inScheme dbc-code:drivers .
    


dbc-code:urban a skos:Concept, sdmx:Concept, dbc-code:Drivers;
    #rdfs:label "Urban"@en;
    skos:topConceptOf dbc-code:drivers ;
    skos:inScheme dbc-code:drivers .



# -- Dimensions  ----------------------------




# -- Attributes  ----------------------------


 :drivers a qb:AttributeProperty ;
  #rdfs:label "Driver Category" ;
  qb:codeList  dbc-code:drivers .




# -- Measures  ----------------------------

:distance  a rdf:Property, qb:MeasureProperty;
    rdfs:label "Euclidean distance"@en;
    rdfs:subPropertyOf sdmx-measure:obsValue;
    owl:sameAs sweet-distance:Distance;
    rdfs:range xsd:decimal ;
    .


# -- Observations  ----------------------------


eg:o11 a qb:Observation;
    sdmx-dimension:refArea ex-geo:newport_00pr ;                  
    :distance          76.7 ;
    :drivers  dbc-code:roads;
    
    .


eg:o12 a qb:Observation;
    sdmx-dimension:refArea ex-geo:newport_00pr ;                  
    :distance          186.7 ;
    :drivers  dbc-code:urban;
    
    .